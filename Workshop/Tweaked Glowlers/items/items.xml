    <Body width="103" height="64" density="15" />
    <Holdable slots="Any,RightHand,LeftHand" controlpose="true" aimpos="70,5" handle1="-35,-7" msg="ItemMsgPickUpSelect" />
    <RangedWeapon barrelpos="60,20" reload="1" spread="5" unskilledspread="15" combatPriority="75" drawhudwhenequipped="true" crosshairscale="0.2">
      <Crosshair texture="Content/Items/Weapons/Crosshairs.png" sourcerect="0,256,256,256" />
      <CrosshairPointer texture="Content/Items/Weapons/Crosshairs.png" sourcerect="256,256,256,256" />
      <ParticleEmitter particle="muzzleflashchaingun" particleamount="1" velocitymin="0" velocitymax="0" scalemin="0.5" scalemax="0.6" />
      <ParticleEmitter particle="explosionsmoke" particleamount="1" velocitymin="0" velocitymax="0" scalemin="0.5" scalemax="0.6" />
      <Sound file="%ModDir%/Workshop/Tweaked Glowlers/items/FlareShot1.ogg" type="OnUse" range="2000" selectionmode="Random" />
      <Sound file="%ModDir%/Workshop/Tweaked Glowlers/items/FlareShot2.ogg" type="OnUse" range="2000" />
      <StatusEffect type="OnUse" target="This">
        <Explosion range="50" force="2" shockwave="false" smoke="false" flames="false" flash="true" sparks="false" underwaterbubble="false" applyfireeffects="false" camerashake="4.0" />
      </StatusEffect>
      <RequiredItems items="flare,alienflare" type="Contained" mincondition="1.0" msg="ItemMsgAmmoRequired" />
      <StatusEffect type="OnUse" target="Contained" identifier="" setvalue="true" />
      <RequiredSkill identifier="weapons" level="40" />
      <SkillRequirementHint identifier="mechanical" level="30" />
    </RangedWeapon>
    <ItemContainer capacity="1" maxstacksize="1" hideitems="true" containedstateindicatorstyle="bullet">
