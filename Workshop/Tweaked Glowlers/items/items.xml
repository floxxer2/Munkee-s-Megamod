  <!--Gun-->
  <Item name="Flare gun" identifier="flaregun" description="Designed for underwater use, it allows to launch flares much further. Launched flares are too hot to pick them up." category="Equipment" cargocontaineridentifier="metalcrate" Tags="smallitem,tool,pistolitem,mountableweapon" Scale="0.4" impactsoundtag="impact_metal_light">
    <PreferredContainer primary="divingcab" minamount="1" maxamount="2" spawnprobability="0.5" notcampaign="true" notpvp="true" />
