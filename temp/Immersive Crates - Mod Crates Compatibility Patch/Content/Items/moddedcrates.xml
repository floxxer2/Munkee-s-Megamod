      <Fabricate suitablefabricators="fabricator" requiredtime="20">
        <RequiredSkill identifier="mechanical" level="20" />
        <Item identifier="steel" amount="2" />
        <Item identifier="lead" amount="2" />
      </Fabricate>
      <InventoryIcon texture="%ModDir:3337851889%/Content/Items/ICMCCP-ItemsAtlas.png" sourcerect="0,84,146,83" origin="0.5,0.5" />
      <Sprite texture="%ModDir:3337851889%/Content/Items/ICMCCP-ItemsAtlas.png" depth="0.54" sourcerect="0,84,146,83" origin="0.5,0.5" />
      <BrokenSprite texture="%ModDir:3337851889%/Content/Items/ICMCCP-ItemsAtlas.png" sourcerect="147,84,106,83" origin="0.5,0.5" depth="0.54" maxcondition="99" fadein="false" />
      <Body width="144" height="80" density="25" />
      <Holdable slots="RightHand+LeftHand" holdangle="5" holdpos="90,-110" handle1="-5,0" handle2="-5,10" aimable="false" msg="ItemMsgPickUpSelect">
        <!-- damage the crate to display the broken sprite and turn the ItemComponent on when it is being held -->
        <StatusEffect type="OnActive" target="This" targetitemcomponent="ItemComponent" IsActive="true" condition="99" setvalue="true" />
        <StatusEffect type="OnActive" target="Character" SpeedMultiplier="0.7" setvalue="true" />
